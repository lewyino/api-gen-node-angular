{
  "$schema": "node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "4.3.1",
    "generators": {
      "angular": {
        "generatorName": "typescript-angular",
        "output": "../openapi",
        "glob": "swagger.json",
        "additionalProperties": {
          "ngVersion": "8.0.0",
          "npmName": "RestApiClient",
          "supportsES6": true,
          "npmVersion": "1.0.0",
          "withInterfaces": true
        }
      }
    }
  }
}
